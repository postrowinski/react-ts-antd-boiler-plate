{
  "name": "boiler_plate",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate",
    "antd",
    "lodash",
    "moment"
  ],
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist",
    "lint": "tslint './src/**/*.ts*' --format stylish --force",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.144",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.2",
    "antd-scss-theme-plugin": "^1.0.8",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.2.0",
    "express": "^4.16.4",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.5.0",
    "less": "^3.9.0",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.8.0",
    "sass-loader": "^7.3.0",
    "style-loader": "^1.0.0",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.3.4000",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "antd": "^3.25.0",
    "react-router-dom": "5.1.2"
  }
}
